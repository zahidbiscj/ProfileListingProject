@model IList<ProfileListingProject.Core.Entities.Project>
@{
    ViewData["Title"] = "Projects";
}

@section styles
{

    <link href="~/css/site.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0/css/bootstrap.css" rel="stylesheet" />
}


@section scripts
{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0/js/bootstrap.min.js"></script>
}


<section id="content">
    <div class="container">
        <div class="row">
            <div class="span12">
                <div class="row">
                    <div class="span3">
                        <button class="btn-danger" onclick="window.history.back();">Return to Previous page</button>
                    </div>
                </div>
                <div class="row">
                    <section id="projects">
                        <ul id="thumbs" class="grid cs-style-4 portfolio">

                            <!-- Item Project and Filter Name -->
                            @foreach (var item in ViewBag.Projects)
                            {
                                <li class="item-thumbs span3 design">
                                    <div class="item">
                                        <figure class="figure">
                                            <div><img src="~/images/@item.ImageUrl" alt="" /></div>
                                            <figcaption>
                                                <div>
                                                    <span>
                                                        <a href="~/images/@item.ImageUrl" data-pretty="prettyPhoto[gallery1]" title="@item.Name"><i class="icon-plus icon-circled icon-bglight icon-2x"></i></a>
                                                    </span>
                                                </div>
                                            </figcaption>
                                        </figure>
                                        <div>
                                            <a class="bold" href="/Company/ProjectView/@item.Id">@item.Name</a>
                                        </div>
                                    </div>
                                </li>
                            }

                            <!-- End Item Project -->
                            <!-- Item Project and Filter Name -->
                            <li class="item-thumbs span3 design" data-id="id-1" data-type="icon">
